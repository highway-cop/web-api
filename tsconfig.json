{
    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "outDir": "dist",
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "noImplicitReturns": true,
        "noImplicitOverride": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "types": [
            "@highway-cop/road-domain"
        ]
    },
}